<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%
	String age = request.getParameter("age");
	//age를 int로 변환 
	try {
		int myage = Integer.parseInt(age);
		out.println("my age: " + myage);
	}catch (Exception e) {
		request.getRequestDispatcher("error01.jsp"); //특정 페이지를 지정해줌
	}
%>